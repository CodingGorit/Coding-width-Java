<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.gorit.entity.Student" table="student" schema="studentmanagementsystem">
        <id name="stuId">
            <column name="stuId" sql-type="varchar(10)"/>
        </id>
        <property name="stuName">
            <column name="stuName" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="stuAge">
            <column name="stuAge" sql-type="int(3)" length="3"/>
        </property>
        <property name="classz">
            <column name="classz" sql-type="varchar(30)" length="30"/>
        </property>
    </class>
</hibernate-mapping>